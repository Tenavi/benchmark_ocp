{#
Extend the default template instead of defining everything ourselves.
#}
{% extends "default/module.html.jinja2" %}

{#
Adjust which members are documented by overriding the is_public macro.
#}
{% macro is_public(doc) %}
    {% if doc.name == "__call__" %}
        true
    {% else %}
        {{ default_is_public(doc) }}
    {% endif %}
{% endmacro %}